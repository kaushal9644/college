<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>TestingDb</servlet-name>
        <servlet-class>controller.TestingDb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>controller.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentDataInsert</servlet-name>
        <servlet-class>controller.StudentDataInsert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherDataInsert</servlet-name>
        <servlet-class>controller.TeacherDataInsert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuickContact</servlet-name>
        <servlet-class>controller.QuickContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginChecker</servlet-name>
        <servlet-class>controller.LoginChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherUpdate</servlet-name>
        <servlet-class>controller.TeacherUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentUpdate</servlet-name>
        <servlet-class>controller.StudentUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminStudentUpdate</servlet-name>
        <servlet-class>controller.AdminStudentUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminTeacherUpdate</servlet-name>
        <servlet-class>controller.AdminTeacherUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgetPassword</servlet-name>
        <servlet-class>controller.ForgetPassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentChangePass</servlet-name>
        <servlet-class>controller.StudentChangePass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherPass</servlet-name>
        <servlet-class>controller.TeacherPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact</servlet-name>
        <servlet-class>controller.Contact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestingDb</servlet-name>
        <url-pattern>/TestingDb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentDataInsert</servlet-name>
        <url-pattern>/StudentDataInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherDataInsert</servlet-name>
        <url-pattern>/TeacherDataInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuickContact</servlet-name>
        <url-pattern>/QuickContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginChecker</servlet-name>
        <url-pattern>/LoginChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherUpdate</servlet-name>
        <url-pattern>/TeacherUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentUpdate</servlet-name>
        <url-pattern>/StudentUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminStudentUpdate</servlet-name>
        <url-pattern>/AdminStudentUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminTeacherUpdate</servlet-name>
        <url-pattern>/AdminTeacherUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgetPassword</servlet-name>
        <url-pattern>/ForgetPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentChangePass</servlet-name>
        <url-pattern>/StudentChangePass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherPass</servlet-name>
        <url-pattern>/TeacherPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact</servlet-name>
        <url-pattern>/Contact</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
